{
	"name": "ushergodwin/phaser",
	"description": "phaser SQL is PHP package that provides convenient PHP and SQL functions such that you focus on the system functionality.",
    "keywords": ["bluefaces", "phaser", "php_sql"],
	"type": "library",
	"require": {
		"vlucas/phpdotenv": "^5.3",
        "ext-pdo": "*",
		"opis/closure": "^3.6",
		"larapack/dd": "^1.1",
		"php": ">= 7.4"
    },
	"license": "MIT",
	"authors": [
		{
			"name": "Tumuhimbise Usher Godwin",
			"email": "godwintumuhimbise96@gmail.com"
		}
	],
	"autoload": {
        "files": [
			"src/functions.php"
		],
		"psr-4": {
			"Phaser\\": "src/"
		}
    },
	"extra": {
        "branch-alias": {
            "dev-master": "3.6.x-dev"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
	"minimum-stability": "dev",
    "prefer-stable": true
}
